<?xml version="1.0" encoding="UTF-8"?>
<Package xmlns="http://soap.sforce.com/2006/04/metadata">
<%- include MetadataHelper -%>
<%- hash.each { |key, value| -%>
	<types>
		<%- value.each do |f| -%>
		<members><%= f %></members>
		<%- end -%>
		<name><%= key %></name>
	</types>
<%- } -%>
	<version><%= MM_API_VERSION %></version>
</Package>